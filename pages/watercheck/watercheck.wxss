page{
  width: 100%;
  height: 100%;
}
.watercheck-one{
  width: 100%;
  height: 90%;
}
.watercheck{
  width: 100%;
  height: 100%;
}

.clearfix:before, .clearfix:after {
  content: "";
  display: table;
}

.clearfix:after {
  clear: both;
}

.clearfix {
  zoom: 1;
}

.watercheck-title{
  width: 100%;
  font-size: 46rpx;
  text-align: center;
  padding-top: 60rpx;
  padding-bottom: 50rpx;
  color: #333333;
}

.watercheck-content{
  width: 100%;
}

.watercon-item{
  width: 80%;
  margin: auto;
  font-size: 30rpx;
  margin-top: 20rpx;
}

.wateritem-title{
  float: left;
  color: #333333;
}

.wateritem-text{
  float: left;
  color: #666666
}